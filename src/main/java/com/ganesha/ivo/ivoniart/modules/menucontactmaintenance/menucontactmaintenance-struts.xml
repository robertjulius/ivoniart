<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="menucontactmaintenance" extends="basicweb"
		namespace="/modules/menucontactmaintenance">

		<!-- =================================== -->
		<!-- MenuContactMaintenanceMainAction : BEGIN -->
		<!-- =================================== -->
		<action name="initial"
			class="com.ganesha.ivo.ivoniart.modules.menucontactmaintenance.action.MenuContactMaintenanceMainAction"
			method="initial">
			<result name="success" type="chain">prepareDetail
			</result>
		</action>

		<action name="prepareDetail"
			class="com.ganesha.ivo.ivoniart.modules.menucontactmaintenance.action.MenuContactMaintenanceMainAction"
			method="prepareDetail">
			<result name="success" type="chain">detail
			</result>
		</action>

		<action name="detail"
			class="com.ganesha.ivo.ivoniart.modules.menucontactmaintenance.action.MenuContactMaintenanceMainAction"
			method="chainAction">
			<result name="success">/jsp/modules/menucontactmaintenance/menu_contact_maintenance_detail.jsp
			</result>
		</action>
		<!-- ================================= -->
		<!-- MenuContactMaintenanceMainAction : END -->
		<!-- ================================= -->

		<!-- ===================================== -->
		<!-- MenuContactMaintenanceUpdateAction : BEGIN -->
		<!-- ===================================== -->
		<action name="prepareUpdate"
			class="com.ganesha.ivo.ivoniart.modules.menucontactmaintenance.action.MenuContactMaintenanceUpdateAction"
			method="prepareUpdate">
			<result name="success" type="chain">formUpdate
			</result>
		</action>

		<action name="formUpdate"
			class="com.ganesha.ivo.ivoniart.modules.menucontactmaintenance.action.MenuContactMaintenanceUpdateAction"
			method="chainAction">
			<result name="success">/jsp/modules/menucontactmaintenance/menu_contact_maintenance_update.jsp
			</result>
		</action>

		<action name="executeUpdate"
			class="com.ganesha.ivo.ivoniart.modules.menucontactmaintenance.action.MenuContactMaintenanceUpdateAction"
			method="executeUpdate">
			<result name="success" type="chain">prepareDetail
			</result>
			<result name="error">/jsp/modules/menucontactmaintenance/menu_contact_maintenance_update.jsp
			</result>
		</action>
		
	</package>

</struts>
